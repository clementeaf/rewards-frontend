<div class="row msr-form">
  <div class="card darken-1">
    <div class="card-content">
      <span class="card-title">
        <div class="col s10">Tarjeta #{{tarjeta.numero}}</div>
        <div class="col s2">{{optional-tarjeta-primaria-label esPrimaria=tarjeta.esPrimaria}}</div>
      </span>
      <div class="row">
        {{tenpines/labeled-label label='Alias' value=tarjeta.alias col='s3'}}
        {{tenpines/labeled-label label='Saldo' value=(as-currency tarjeta.saldo) col='s3'}}
        {{tenpines/labeled-label label='Instante de ultima recarga' value=(as-datetime tarjeta.instanteDeLaUltimaRecarga) col='s3'}}
        <div class="col s3">
          {{estado-tarjeta-label estado=tarjeta.estado}}
        </div>
        <div class="col s3">
          {{vencimiento-tarjeta-label label='vencimiento ' tarjeta=tarjeta}}
        </div>
      </div>
      <br>
      {{detalle-de-perfil-autorecarga perfil=tarjeta.perfilDeAutoRecarga}}
      <br>
    </div>
  </div>
</div>